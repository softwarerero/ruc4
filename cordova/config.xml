<?xml version='1.0' encoding='utf-8'?>
<widget id="py.com.sun.ruc" version="4.0.1" android-versionCode="23" ios-CFBundleVersion="4.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RUCParaguay</name>
    <description>Buscar un RUC de Paraguay</description>
    <author email="info@sun.com.py" href="http://sun.com.py">SUNCOM Systems</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <!--<allow-intent href="tel:*" />-->
    <!--<allow-intent href="sms:*" />-->
    <!--<allow-intent href="mailto:*" />-->
    <!--<allow-intent href="geo:*" />-->
    <platform name="android">
      <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
      <allow-intent href="itms:*" />
      <allow-intent href="itms-apps:*" />
    </platform>

  <platform name="android">
    <icon src="www/img/android/mipmap-xxxhdpi/ic_launcher.png"/>
    <splash src="www/img/android/mipmap-xxxhdpi/ic_launcher.png" density="port-xxxhdpi"/>
  </platform>
  
  <platform name="ios">
    <icon src="www/img/ios/AppIcon.appiconset/Icon-40.png" width="40" height="40" />
    <icon src="www/img/ios/AppIcon.appiconset/Icon-40@2x.png" width="80" height="80" />
    <icon src="www/img/ios/AppIcon.appiconset/Icon-40@3x.png" width="120" height="120" />
    <icon src="www/img/ios/AppIcon.appiconset/Icon-60@3x.png" width="180" height="180" />
    <icon src="www/img/ios/AppIcon.appiconset/Icon-76.png" width="76" height="76" />
    <icon src="www/img/ios/AppIcon.appiconset/Icon-76@2x.png" width="152" height="152" />
    <icon src="www/img/ios/AppIcon.appiconset/Icon-83.5@2x.png" width="167" height="167" />
    <icon src="www/img/ios/AppIcon.appiconset/Icon-Small.png" width="29" height="29" />
    <icon src="www/img/ios/AppIcon.appiconset/Icon-Small@2x.png" width="58" height="58" />
    <icon src="www/img/ios/AppIcon.appiconset/Icon-Small@3x.png" width="87" height="87" />
  </platform>
  
</widget>
